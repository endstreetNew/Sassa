@page "/filerequest"
@inject SessionService sessionservice
<BrmPageTitle Title="File requests" />

<FluentTabs>
    <FluentTab Label="Request Files" Id="frTab-1">
        <SearchFiles></SearchFiles>
    </FluentTab>
    <FluentTab Label="View Requests" Id="frTab-2">
        <ViewRequests></ViewRequests>
    </FluentTab>
    <FluentTab Label="Picklist Progress" Id="frTab-3" Disabled="@(!sessionservice.session.IsRmc())">
        <Picklist></Picklist>
    </FluentTab>
</FluentTabs>


